<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>Hierarchy</customSettingsType>
    <description>This custom setting is used for activating and deactivating the validation rules as required.</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Activate_Validation_Rule__c</fullName>
